apiVersion: cloudbuild.cnrm.cloud.google.com/v1beta1
kind: CloudBuildWorkerPool
metadata:
  annotations:
    cnrm.cloud.google.com/management-conflict-prevention-policy: none
  name: cloudbuildworkerpool-${uniqueId}
  namespace: ${uniqueId}
spec:
  displayName: New CloudBuild WorkerPool
  location: us-central1
  privatePoolV1Config:
    networkConfig:
      egressOption: NO_PUBLIC_EGRESS
      peeredNetworkIPRange: /29
      peeredNetworkRef:
        external: projects/${projectId}/global/networks/computenetwork-${uniqueId}
    workerConfig:
      diskSizeGb: 100
      machineType: e2-medium
  projectRef:
    name: ${projectId}
  resourceID: projects/${projectId}/locations/us-central1/workerPools/cloudbuildworkerpool-${uniqueId}