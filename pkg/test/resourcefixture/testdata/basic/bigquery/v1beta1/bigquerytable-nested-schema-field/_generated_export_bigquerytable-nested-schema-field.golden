apiVersion: bigquery.cnrm.cloud.google.com/v1beta1
kind: BigQueryTable
metadata:
  annotations:
    cnrm.cloud.google.com/project-id: ${projectId}
  labels:
    cnrm-test: "true"
    managed-by-cnrm: "true"
  name: bigquerytablesample${uniqueId}
spec:
  datasetRef:
    external: bigquerydatasetsample${uniqueId}
  description: Table containing policy history updated
  friendlyName: bigquerytable-sample-updated
  resourceID: bigquerytablesample${uniqueId}
  schema: '[{"description":"Fully qualified name of the entity","mode":"REQUIRED","name":"qualified_name","type":"STRING"},{"description":"Name
    of the entity","mode":"REQUIRED","name":"name","type":"STRING"},{"description":"Unique
    identifier of the entity","mode":"REQUIRED","name":"guid","type":"STRING"},{"description":"List
    of Atlan Tags associated with the entity","fields":[{"description":"Name of Atlan
    Tag","mode":"NULLABLE","name":"type_name","type":"STRING"},{"description":"GUID
    of the entity for this tag","mode":"NULLABLE","name":"entity_guid","type":"STRING"},{"description":"Status
    of the entity","mode":"NULLABLE","name":"entity_status","type":"STRING"}],"mode":"REPEATED","name":"atlan_tags","type":"RECORD"},{"description":"atlan
    BQ labels","fields":[{"description":"key","mode":"NULLABLE","name":"key","type":"STRING"},{"description":"values","mode":"NULLABLE","name":"value","type":"STRING"}],"mode":"REPEATED","name":"labels","type":"RECORD"},{"description":"Name
    of the entity","mode":"REQUIRED","name":"type_resource","type":"STRING"},{"description":"Last
    update time of the entity","mode":"REQUIRED","name":"update_time","type":"TIMESTAMP"},{"description":"time
    when cdp get data","mode":"REQUIRED","name":"atlan_get_time","type":"TIMESTAMP"}]'
  timePartitioning:
    field: atlan_get_time
    type: DAY