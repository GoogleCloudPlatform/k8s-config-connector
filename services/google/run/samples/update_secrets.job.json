{
  "name": "{{name}}",
  "project": "run-v2-deploy-e2e",
  "location": "{{region}}",
  "launchStage": "BETA",
  "template": {
    "template": {
      "volumes": [ {
        "name": "my-secret-volume",
        "secret": {
          "secret": "dclsecret",
          "items": [{
            "version": "1",
            "path": "notthelatest"
          }]
        }
      } ],
      "containers": [ {
        "image": "gcr.io/run-intro/screenshot:latest",
        "volumeMounts": [ {
          "name" : "my-secret-volume",
          "mountPath": "/var/foo/secrets"
        } ]
      } ]
    }
  }
}
