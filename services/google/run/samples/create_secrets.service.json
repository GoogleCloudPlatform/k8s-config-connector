{
  "name": "{{name}}",
  "project": "run-v2-deploy-e2e",
  "location": "{{region}}",
  "ingress": "INGRESS_TRAFFIC_ALL",
  "launchStage": "GA",
  "template": {
    "scaling": {
      "maxInstanceCount": 2
    },
    "volumes": [ {
      "name": "my-secret-volume",
      "secret": {
        "secret": "dclsecret",
        "defaultMode": 1,
        "items": [{
          "version": "latest",
          "path": "latest",
          "mode": 1
        }]
      }
    } ],
    "containers": [ {
      "image": "gcr.io/cloudrun/hello",
      "env": [ {
        "name": "FOO",
        "value": "bar"
      }, {
        "name": "latestdclsecret",
        "valueSource": {
          "secretKeyRef" : {
            "secret": "dclsecret",
            "version": "1"
          }
        }
      } ],
      "volumeMounts": [ {
        "name" : "my-secret-volume",
        "mountPath": "/var/foo/secrets"
      } ]
    } ]
  },
  "traffic": [ {
    "type": "TRAFFIC_TARGET_ALLOCATION_TYPE_LATEST",
    "percent": 100
  } ]
}
